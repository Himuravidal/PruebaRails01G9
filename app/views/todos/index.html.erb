<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>Index</title>
  </head>
  <body>
    <%= @todos.each do |todo| %>
    <div class="well"> <%=todo.completed%> <%= link_to todo.description, todo %> <%= link_to "edit", edit_path(todo) %> <%= link_to "Delete", todo, method: :delete %></div>
    <% end %>
    <%= link_to'Crear TO-DO', todos_new_path %>
  </body>
</html>
